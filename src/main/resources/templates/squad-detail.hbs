{{#partial "content"}}
    <p>Squad Name:</p>
    <p>{{ squad.squadName }}</p>
    <h5>Created At:</h5>
    <p> {{ squad.createdAt }}</p>
    <h5>Squad status:</h5>
    <p>
        {{#if squad.published }}
            <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
        {{else}}
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
        {{/if}}
    </p>
{{/partial}}
{{> layout.hbs}}