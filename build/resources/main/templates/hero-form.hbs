{{#partial "content"}}

    <h1>Add Hero!</h1>

    <form action="created-hero" method="post" class="well">
        <label for="heroName">Heros Name</label><br>
        <input id="heroName" name="heroName" type="text" class="input-field">
        <br>

        <label for="age">Heros Age</label><br>
        <input id="age" name="age" type="text" class="input-field">
        <br>

        <label for="strength">Heros Power</label><br>
        <input id="strength" name="strength" type="text" class="input-field">
        <br>

        <label for="weakness">Heros Weakness</label><br>
        <input id="weakness" name="weakness" type="text" class="input-field">
        <br>

        <button type="submit" class="btn btn-default">Add Hero</button>
    </form>
{{/partial}}

{{> layout.hbs}}

