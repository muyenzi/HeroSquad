{{#partial "content"}}

    <h1>Add a new Squad!</h1>

    {{#if squads}}
        {{#each squads }}
            <p>Current squads:</p>
            <p>{{squad.squadName }} </p>
            <p>{{ description }} </p>
            <p>{{ size }} </p>
        {{/each}}
    {{else}}

    {{/if}}
    {{#if heros}}
        {{#each heroes }}
            <p>heroes:</p>
            <p>{{hero.heroName}} </p>
            <p>{{hero.age}} </p>
            <p>{{hero.strength }} </p>
            <p>{{ hero.weakness }} </p>
        {{/each}}
    {{else}}

    {{/if}}
{{/partial}}

{{> layout.hbs}}